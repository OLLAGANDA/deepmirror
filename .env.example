# Database Configuration
POSTGRES_DB=your_database_name
POSTGRES_USER=your_database_user
POSTGRES_PASSWORD=your_database_password

# Spring Datasource Configuration
# Docker Compose 사용 시 'db'는 docker-compose.yml의 서비스명입니다
SPRING_DATASOURCE_URL=jdbc:postgresql://db:5432/your_database_name
SPRING_DATASOURCE_USERNAME=your_database_user
SPRING_DATASOURCE_PASSWORD=your_database_password

# Gemini AI API Key (https://aistudio.google.com/app/apikey)
# 반드시 실제 API 키로 변경하세요
GEMINI_API_KEY=your_gemini_api_key_here

# Discord Webhook URL (피드백 알림)
# Discord 서버 설정 > 연동 > 웹후크에서 생성한 URL을 입력하세요
DISCORD_WEBHOOK_URL=https://discord.com/api/webhooks/YOUR_WEBHOOK_ID/YOUR_WEBHOOK_TOKEN

# JPA Configuration (환경별 설정)
# 개발: update, 프로덕션: validate 또는 none
JPA_DDL_AUTO=update

# Cloudflare Tunnel
CLOUDFLARE_TUNNEL_TOKEN=your_cloudflare_tunnel_token_here